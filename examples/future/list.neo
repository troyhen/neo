trait OrderedList[T]

    def append item~T
        add item

    def prepend item~T
        add item

    def << item~T
        append item

    def :+ item~T
        append item

    def :+ list~List[T]
        append list

    def :: item~T
        prepend item

    def ? item~T
        contains item


