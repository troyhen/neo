trait OrderedList(T) {

    append:(item:T)
        add item
    end

    prepend:(item:T)
        add item
    end

    `<<`:(item:T)
        append item
    end

    `:+`:(item:T)
        append item
    end

    `:+`:(list:List[T])
        append list
    end

    `::`:(item:T)
        prepend item
    end

    `?`:(item:T)
        contains item
    end
}

list = String.new with List
